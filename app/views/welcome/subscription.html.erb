        <meta property="fbpayment:trial_duration" content="2 weeks" />
        <meta property="fb:app_id" content="<%= FB_APP_ID %>" />
        <meta property="og:url" content="<%= subscription_url %>" />
        <meta property="og:type" content="fbpayment:subscription" />
    </head>
</html>
